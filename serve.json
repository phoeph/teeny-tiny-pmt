{
  "cleanUrls": false,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/projects",
      "destination": "/projects.html"
    },
    {
      "source": "/project",
      "destination": "/project.html"
    }
  ],
  "headers": [
    {
      "source": "**/*.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    }
  ]
}